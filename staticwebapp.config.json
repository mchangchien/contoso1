{
    "routes": [
        {
          "route": "/*",
          "allowedRoles": ["weirduser"]
        }
      ],
      "responseOverrides": {
        "401": {
          "statusCode": 302,
          "redirect": "/.auth/login/aad"
        }
      },
    "navigationFallback": {
        "rewrite": "/index.html"
    }
}